/var/log/rails/*log {
    size 10M
    rotate 4
    missingok
    notifempty
    nocompress
    sharedscripts
    postrotate
    /bin/kill -HUP `cat /var/run/syslogd.pid 2> /dev/null` 2> /dev/null || true
    endscript
}